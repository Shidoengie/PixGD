}

[application]

config/name="Pixgd"
run/main_scene="res://scenes/VImgScene.tscn"
config/icon="res://icon.png"

[display]

mouse_cursor/custom_image="res://assets/cursor.png"

[editor_plugins]

enabled=PoolStringArray( "res://addons/SliderLabel/plugin.cfg" )

[gdnative]

singletons=[ "res://addons/godot-git-plugin/git_api.gdnlib" ]

