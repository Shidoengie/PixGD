; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Node2D",
"class": "Example1",
"language": "GDScript",
"path": "res://addons/ezd/examples_src/example1.gd"
}, {
"base": "KinematicBody2D",
"class": "Example2",
"language": "GDScript",
"path": "res://addons/ezd/examples_src/example2.gd"
}, {
"base": "Sprite",
"class": "Example3",
"language": "GDScript",
"path": "res://addons/ezd/examples_src/example3.gd"
}, {
"base": "Node",
"class": "Ezd",
"language": "GDScript",
"path": "res://addons/ezd/src/ezd.gd"
}, {
"base": "",
